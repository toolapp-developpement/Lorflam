<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <form id="mrp-form-custom-extend" name="mrp-form" title="MRP"
    model="com.axelor.apps.supplychain.db.Mrp" width="large" extension="true">
    <extend target="//panel-related[@name='productFamilySetPanel']">
      <insert position="after">
        <panel-related name="suppliersSetPanel" colSpan="12" field="suppliersSet"
          form-view="partner-supplier-form" grid-view="partner-supplier-grid" canNew="false"
          canEdit="false" domain="self.isSupplier = true"/>
      </insert>
    </extend>
  </form>
</object-views>