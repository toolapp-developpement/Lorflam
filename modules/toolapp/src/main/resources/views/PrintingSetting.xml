<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <form id="printing-setting-form-custom" name="printing-setting-form" title="Printing Setting"
    model="com.axelor.apps.base.db.PrintingSettings" width="large" extension="true">
    <extend target="//panel[@name='companyFooterPanel']">
      <insert position="after">
        <panel name="ecoTaxDetails" title="Modèle Eco Participation" colSpan="12">
          <field name="ecoTaxTemplate" title="Modèle Eco Participation Commande"/>
          <field name="ecoTaxInvoiceTemplate" title="Modèle Eco Participation Facture"/>
        </panel>
      </insert>
    </extend>

  </form>


</object-views>
