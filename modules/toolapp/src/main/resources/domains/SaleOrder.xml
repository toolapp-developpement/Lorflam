<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="sale" package="com.axelor.apps.sale.db"/>

  <entity name="SaleOrder" lang="java">
    <one-to-many name="invoiceEcoTaxDetailList"
      ref="com.axelor.apps.toolapp.db.InvoiceEcoTaxDetail" title="Invoice eco tax detail list"
      mappedBy="saleOrder"/>
    <string name="ecoTaxDetails" title="Eco tax details" large="true"/>
  </entity>

</domain-models>