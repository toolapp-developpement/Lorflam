<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="toolapp" package="com.axelor.apps.toolapp.db"/>

  <entity name="InvoiceEcoTaxDetail" lang="java">
    <many-to-one name="invoice" ref="com.axelor.apps.account.db.Invoice" title="Invoice"/>
    <many-to-one name="saleOrder" title="Sale order"
      ref="com.axelor.apps.sale.db.SaleOrder"/>
    <many-to-one name="ecoTax" ref="com.axelor.apps.toolapp.db.EcoTax" title="Eco tax"/>
    <decimal name="amount" title="Amount"/>
  </entity>

</domain-models>
